<h1>Songs</h1>

<table>
  <thead>
    <th>Song Title</th>
    <th>Release Year</th>
    <th>Artist's Name</th>
    <th>Genre</th>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
    <% end %>
  </tbody>
</table>

